{
  "name": "project.io",
  "version": "1.0.0",
  "description": "firstpush",
  "main": "gulpfile.js",
  "scripts": {
    "prestart": "mkdir -p build/js/min && mkdir -p build/css/min",
    "start": "lessc src/less/index.less build/css/index.css && uglifycss build/css/index.css > build/css/min/index.css",
    "poststart": "browserify -t vueify -e src/js/main.js -o build/js/main.js -t [ babelify ] && uglifyjs build/js/main.js -o build/js/min/main.js "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lincerossa/project.io.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Lincerossa/project.io/issues"
  },
  "homepage": "https://github.com/Lincerossa/project.io#readme",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "less": "^2.7.1",
    "uglify-js": "^2.7.5",
    "vue": "^2.1.6",
    "vueify": "^9.4.0"
  }
}
